<?xml version="1.0" encoding="UTF-8"?>
<!--<interface type="import" model="">-->
<interface type="import" model="sales_order">
  <fetching>10</fetching>
  <observers>
    <observer>Observer</observer>
  </observers>
  <in>
    <connection type="soap">
<!--      <debug>1</debug>-->
      <url>{crosslog/api_test/url_root}/CustomerOrderExport.asmx?wsdl</url>
      <useLogin>0</useLogin>
      <header>
        <namespace>ns1</namespace>
        <name>AuthHeader</name>
        <params>
          <Username>{crosslog/api_test/user}</Username>
          <Password>{crosslog/api_test/password}</Password>
        </params>
      </header>
      <call>__soapCall</call>
      <callparams>
          <action>GetCustomerOrdersChanged</action>
      </callparams>
      <behavior>3</behavior>
		</connection>
		<format type="xml"/>
  </in>
  <out>
    <connection type="mage">
      <path>{jaymard_ecpm/mage/path}</path>
      <apiclass>Ecpm_Crosslog_Model_Sales_Order_Api</apiclass>
      <apimethod>updateFromCrosslog</apimethod>
		</connection>
		<format type="array"/>
  </out>
</interface>