<?xml version="1.0" encoding="UTF-8"?>
<interface type="import" model="product">
	<logging/>
  <observers>
    <observer>Observer</observer>
  </observers>	
	<in fetching="20">
		<connection type="file">
				<dir>{DIR_ROOT}var/feeds/colombus/</dir>
				<file>fa.paq</file>
		</connection>
		<format type="csvSequenced">
      <separator>C_TAB</separator>
      <headline >0</headline>
      
      <sequence>FA,PR,NCM,EI</sequence>
      
    </format>
	</in>
	<out fetching="10">
		<connection type="soap">
      <url >{ocpm/soap/api2_url}</url>
      <user>{ocpm/soap/user}</user>
      <key >{ocpm/soap/key}</key>
      <call>catalogProductCreate</call>
      <callparams>
          <session>{session}</session>
      </callparams>                
		</connection>
		<format type="array"/>
	</out>
</interface>