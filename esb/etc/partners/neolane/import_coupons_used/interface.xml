<?xml version="1.0" encoding="UTF-8"?>
<interface type="import" model="" memoryLimit="2048">
    <observers>
        <observer>EaiObserverStandard</observer>
    </observers>
    <in fetching="500">
        <connection type="file">
            <workflow>0</workflow>
            <filePattern>^NEO_OCP_BON_USED.*.csv$</filePattern>
<!--             <plugins>
                <plugin type="ftp">
                    <server>{neolane/ftp/server}</server>
                    <user>{neolane/ftp/user}</user>
                    <password>{neolane/ftp/password}</password>
                    <override>{neolane/ftp/override}</override>
                    <remoteDir>/import_customer_cdm/</remoteDir>
                </plugin>
            </plugins> -->
        </connection>
        <format type="csv">
            <separator>C_SEMICOL</separator>
            <enclosure>C_QUOTE</enclosure>
            <headline >1</headline>
        </format>
    </in>
    <out fetching="500">
        <connection type="mage">
            <path>{dpam/mage/path}</path>
            <apiclass>Dpam_Neolane_Model_SalesRule_Coupon_Api</apiclass>
            <apimethod>update</apimethod>
        </connection>
    </out>
