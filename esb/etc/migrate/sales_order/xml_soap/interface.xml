<?xml version="1.0" encoding="UTF-8"?>
<interface type="import" model="sales_order">
	<fetching>10</fetching>
  <in>
    <connection type="xml">
        <debug>1</debug>
        <dir>{DIR_ROOT}var/feeds/in</dir>
        <file>dpam_orders.xml</file>
    </connection>
    <format type="xml">
    </format>
  </in>
	<out>
		<connection type="soap">
        <debug>1</debug>
        <url>{ecpm/soap/api_url}</url>
        <user>{ecpm/soap/user}</user>
        <key>{ecpm/soap/key}</key>
        <callparams>
            <action>customer.list</action>
        </callparams>
		</connection>
		<format type="array"/>
	</out>
</interface>
