<?xml version="1.0" encoding="UTF-8"?>
<interface type="import" model="product">
	<logging/>
	<observers>
		<observer>ObserverSample</observer>
	</observers>
	<in>
		<connection type="file">
				<file>products.csv</file>
		</connection>
		<format type="csv"/>
