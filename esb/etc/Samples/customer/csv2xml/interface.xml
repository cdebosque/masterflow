<?xml version="1.0" encoding="UTF-8"?>
<interface type="export" model="samples_customer">
	<fetching>10</fetching>
	<logging/>
	<observers>
		<observer>Observer</observer>
	</observers>
	<filters>
		<filter>
			<name type="text">RS_*</name>
			<marge type="decimal">>10.2</marge>
			
		</filter>
	</filters>
	<in>
		<connection type="file">
				<file>exemple-1-clients.csv</file>
		</connection>
		<format type="csv">
    	    <headline >1</headline>
    	    <separator>C_SEMICOL</separator>
            <enclosure>C_QUOTE</enclosure>
    	  </format>
    	  
    	  <mapping active="0"/>
	</in>
	<out>
		<connection type="file">
            <mode>w+</mode>
            <file>exemple-1-clients.xml</file>
		</connection>
		<format type="xml">
			<xmlNodeRoot>customer</xmlNodeRoot>
        </format>
        
	</out>
</interface>