<?xml version="1.0" encoding="UTF-8"?>
<interface type="import" model="category">
 	<!-- <logging/> -->
 	<observers>
		<observer>Observer</observer>
	</observers>
    <in fetching="50">
        <connection type="file">
                <file>dpam_mapping_categories_websites.csv</file>
        </connection>
        <format type="csv"/>
    </in>
	<out fetching="0">
		<connection type="soap">
                <!-- <url>http://dpam-recette1.oclio.com/index.php/api/?wsdl</url>-->
                <url>{dpam_recette/soap/api_url}</url> -->
                <user>{ocpm_api/soap/user}</user>
                <key>{ocpm_api/soap/key}</key>
                <call>call</call>
                <callparams>
                    <session>{session}</session>
                    <action>colombus_category.transfertAssignedProducts</action>
                </callparams>
		</connection>
		<format type="array"/>
	</out>
</interface>