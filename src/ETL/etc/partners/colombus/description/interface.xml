<?xml version="1.0" encoding="UTF-8"?>
<interface type="import" model="product">
    <observers>
        <observer>Observer</observer>
    </observers>	
	<in fetching="1">
		<connection type="file">
				<workflow>1</workflow>
				<filePattern>\.csv$</filePattern>
				<ending>C_EOL</ending>
				<csv>true</csv>
		</connection>
		<format type="csv">
				<encoding>utf-8</encoding>
				<separator>;</separator>
				<headline>1</headline>
				<enclosure>"</enclosure>
		</format>
	</in>
<!--     <out mode="block"> -->
    <out>
         <connection type="mage">
                <path>{dpam/mage/path}</path>
                <apiclass>Dpam_Colombus_Model_Product_Api</apiclass>
                <apimethod>updateProductLocales</apimethod>
        </connection>
        <format type="array"/>     
    </out>
<!--
    <out fetching="0">
        <connection type="soap">
                <url>{dpam/soap/api_url}</url>
                <user>{dpam/soap/user}</user>
                <key>{dpam/soap/key}</key>
                <call>call</call>
                <callparams>
                    <session>{session}</session>
                    <action>colombus_product.updateProductLocales</action>
                </callparams>
        </connection>
    	<format type="array"/>
    </out>
-->
</interface>
