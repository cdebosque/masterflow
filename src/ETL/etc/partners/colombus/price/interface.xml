<?xml version="1.0" encoding="UTF-8"?>
<interface type="import" model="product">
  <observers>
    <observer>Observer</observer>
  </observers>
	<in fetching="1000">
		<connection type="file">
				<filePattern>^pv.*\.paq$</filePattern>
        <file>pv_test.paq</file>
		</connection>
		<format type="csv">
      <separator>C_TAB</separator>
      <headline >0</headline>
    </format>
	</in>
    
    <out fetching="0">
        <connection type="mage">
          <path>{dpam/mage/path}</path>
          <apiclass>Dpam_Colombus_Model_Product_Api</apiclass>
          <apimethod>updatePrices</apimethod>
        </connection>
        <format type="array"/>
    </out>    
    
