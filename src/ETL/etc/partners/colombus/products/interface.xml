<?xml version="1.0" encoding="UTF-8"?>
<interface type="import" model="product">
    <observers>
        <observer>Observer</observer>
    </observers>
    <in fetching="5">
        <connection type="file">
            <filePattern>.*\.paq$</filePattern>
            <workflow>0</workflow>
        </connection>
        <format type="csvSequenced">
            <separator>C_TAB</separator>
            <headline>0</headline>

            <sequence>FA,PR,NCM,EI</sequence>

        </format>
    </in>
    
    <out fetching="0">
        <connection type="mage">
          <path>{dpam/mage/path}</path>
          <apiclass>Mage_Catalog_Model_Product_Api_V2</apiclass>
          <apimethod>create</apimethod>
        </connection>
        <format type="array"/>
    </out>  
    
    
