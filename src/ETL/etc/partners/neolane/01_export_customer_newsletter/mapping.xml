<?xml version="1.0" encoding="UTF-8"?>
<mapping>
    <out>
        <fields>

            <field>
                <from>cdm_card_number</from>
                <to>B_CARTEFID</to>
            </field>
            <field>
                <from>uid</from>
                <to>B_INTERNET_ID</to>
            </field>
            <field>
                <from>uid</from>
                <to>B_NEWSLETTERID</to>
            </field>
            <field>
                <from>country</from>
                <to>B_PAYS</to>
            </field>
            <field>
                <to>B_INTERNET_LANG</to>
                <constant>FR</constant>
            </field>
            <field>
                <from>gender</from>
                <to>B_GENDER</to>
                <calls>
                    <call name="getAssociatedValue">
                        <file>transco_gender_mage2neolane.csv</file>
                        <path></path>
                    </call>
                </calls>
            </field>
            <field>
                <from>firstname</from>
                <to>B_FIRSTNAME</to>
            </field>
            <field>
                <from>lastname</from>
                <to>B_LASTNAME</to>
            </field>
            <field>
                <from>dob</from>
                <to>B_BIRTHDATE</to>
                <calls>
                    <call name="replace">
                        <search>0000-00-00</search>
                    </call>
                    <call name="dateYMD2DMY"></call>
                </calls>
            </field>
            <field>
                <from>telephone</from>
                <to>B_PHONE_2</to><!-- [au lieu de PHONE_INT] -->
            </field>
            <field>
                <from></from>
                <to>B_PHONE_3</to><!-- [au lieu de PHONE_NL] -->
                <constant></constant>
            </field>
            <field>
                <from>telephone</from>
                <to>B_CELLPHONE_2</to><!-- [au lieu deCELLPHONE_INT] -->
            </field>
            <field>
                <from></from>
                <to>B_CELLPHONE_3 </to><!-- [au lieu de CELLPHONE_NL] -->
                <constant></constant>
            </field>
            <field>
                <from>email</from>
                <to>B_EMAIL</to>
            </field>
            <field>
                <from>street</from>
                <to>B_ADRESSE1</to>
            </field>
            <field>
                <from></from>
                <to>B_ADRESSE2</to>
                <constant></constant>
            </field>
            <field>
                <from></from>
                <to>B_ADRESSE3</to>
                <constant></constant>
            </field>
            <field>
                <from></from>
                <to>B_ADRESSE4</to>
                <constant></constant>
            </field>
            <field>
                <from></from><!-- TODO billing_postcode + billing_city -->
                <to>B_ADRESSE5</to>
                <constant></constant>
            </field>
            <field>
                <from>country</from>
                <to>B_ADRESSE6</to><!-- pays -->
            </field>
            <field>
                <from>postcode</from>
                <to>B_CODE_POSTAL</to>
            </field>
            <field>
                <from>city</from>
                <to>B_VILLE</to>
            </field>
            <field>
                <from></from>
                <to>B_INTERNET_NEWSLETTER</to>
                <constant></constant>
            </field>
            <field>
                <from>entity_id</from>
                <to>B_INTERNET_PARTNER</to>
                <constant></constant>
            </field>
            <field>
                <from></from>
                <to>B_NEWSLETTER_NEWSLETTER</to>
                <constant></constant>
            </field>
            <field>
                <from></from>
                <to>B_NEWSLETTER_PARTNER</to>
                <constant></constant>
            </field>
            <field>
                <from></from>
                <to>B_NEWSLETTER_SMS</to>
                <constant></constant>
            </field>
            <field>
                <from></from>
                <to>B_CREE_PAR</to>
                <constant></constant>
            </field>
            <field>
                <from>created_at</from>
                <to>B_CREE_DATE</to>
                <calls>
                    <call name="replace">
                        <search>0000-00-00</search>
                    </call>
                    <call name="dateYMD2DMY"></call>
                </calls>
            </field>

        </fields>
    </out>
</mapping>