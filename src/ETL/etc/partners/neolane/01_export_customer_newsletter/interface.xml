<?xml version="1.0" encoding="UTF-8"?>
<interface type="export" memoryLimit="4096">
    <observers>
        <observer>EaiObserverStandard</observer>
    </observers>
    <in fetching="100">
        <connection type="mage">
            <path>{dpam/mage/path}</path>
            <apiclass>Dpam_Neolane_Model_Newsletter_Subscriber_Api</apiclass>
            <apimethod>listCustomersAndSubscribers</apimethod>
        </connection>
        <mapping active="0"/>
    </in>
    <out fetching="100">
        <connection type="file">
            <workflow>1</workflow>
            <file>NEOLANE_CLIENTS_{php::date(YmdHis)}.csv</file>
            <!-- /home/customers/dpam/incoming/import/ -->
            <plugins>
                <plugin type="ftp">
                    <server>{neolane/ftp/server}</server>
                    <user>{neolane/ftp/user}</user>
                    <password>{neolane/ftp/password}</password>
                    <override>{neolane/ftp/override}</override>
                    <remoteDir>/export_customer_newsletter/</remoteDir>
                </plugin>
            </plugins>
        </connection>
        <format type="csv">
            <separator>C_SEMICOL</separator>
            <enclosure>C_QUOTE</enclosure>
            <headline>1</headline>
        </format>
        <mapping active="0"/>
    </out>
</interface>