<?xml version="1.0" encoding="UTF-8"?>
<interface type="import">
  <observers>
    <observer>Observer</observer>
  </observers>
  <in fetching="1">
  <connection type="file">
    <mapping active="0"/>
    <dir>{DIR_ROOT}var/feeds/in/crosslog/</dir>
    <file>Init_Stock.csv</file>
  </connection>
  <format type="csv"></format>
  </in>
  <out fetching="1">
    <connection type="soap">
      <debug>0</debug>
      <url>{dpam/crosslog/api_prod/url_root}/ProductRepositoryImport.asmx?wsdl</url>
      <useLogin>0</useLogin>
      <header>
        <namespace>ns1</namespace>
        <name>AuthHeader</name>
        <params>
          <Username>{dpam/crosslog/api_prod/user}</Username>
          <Password>{dpam/crosslog/api_prod/password}</Password>
        </params>
      </header>
      <call>__doRequest</call>
      <callparams>
          <action>Create</action>
          <urlAction>http://ws.crossdesk.com</urlAction>
      </callparams>
      <behavior>3</behavior>
		</connection>
		<format type="array"/>
  </out>
</interface>
