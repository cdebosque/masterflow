<?xml version="1.0" encoding="UTF-8"?>
<interface type="import" model="product_attributes_labels">
	<logging/>
	<observers>
		<observer>Observer</observer>
	</observers>
    <in>
        <connection type="file">
                 <file>ocp_catalog_product_attributes_labels.csv</file>
        </connection>
        <format type="csv">
            <separator>,</separator>
        </format>
        <mapping active="1"/>
    </in>
	<out>
		<connection type="soap">
                <url>{ocpm/soap/api_url}</url>
                <user>{ocpm/soap/user}</user>
                <key>{ocpm/soap/key}</key>
				<action>product_attribute.update</action>
		</connection>
		<format type="array"/>
	</out>
</interface>